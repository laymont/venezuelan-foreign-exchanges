{
    "name": "laymont/venezuelan-foreign-exchanges",
    "description": "Laravel package for fetching and managing Venezuelan foreign exchange rates",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Laymont\\VenezuelanForeignExchanges\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Laymont",
            "email": "laymont@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "symfony/css-selector": "^7.2",
        "symfony/dom-crawler": "^7.2",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laymont\\VenezuelanForeignExchanges\\Providers\\VenezuelanForeignExchangesServiceProvider"
            ]
        }
    }
}
